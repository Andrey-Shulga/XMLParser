<?xml version="1.0" encoding="UTF-8"?>
<tariffs xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:noNamespaceSchemaLocation="tariff.xsd">

    <bean>
        <class>com.epam.as.xmlparser.entity.FeeTariff</class>
        <property>
            <name>id</name>
            <value>
                <integer>1</integer>
            </value>
        </property>
        <property>
            <name>name</name>
            <value>
                <string>All for 500!</string>
            </value>
        </property>
        <property>
            <name>fee</name>
            <value>
                <integer>500</integer>
            </value>
        </property>
        <property>
            <name>includedMinutes</name>
            <value>
                <integer>50</integer>
            </value>
        </property>
        <property>
            <name>includedTraffics</name>
            <value>
                <integer>500</integer>
            </value>
        </property>
    </bean>


</tariffs>